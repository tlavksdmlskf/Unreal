#include <stdio.h>

int main()
{

	int input;

	printf("1. 밥 \n");
	printf("2. 목욕 \n");
	printf("3. 잠 \n");
	printf("4. 종료 \n");

	scanf("%d", &input);

	switch (input)
	{
		case 1:
			printf("맛나네");
			break;

		case 2:
			printf("시원하다");
			break;

		case 3:
			printf("Zzz");
			break;

		default:
			printf("?");
			break;
	}
	return 0;
}
